{
  "name": "sk-template",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "bun run cli:build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepare": "husky",
    "i18n:check": "bun scripts/checkTranslations.ts",
    "db:migrate": "bun scripts/db/migrate.ts",
    "db:create-migration": "bun scripts/db/createMigration.ts",
    "db:make-admin": "bun scripts/db/makeAdmin.ts",
    "cli:build": "tsc -p cli/tsconfig.json",
    "cli:run": "bun cli/dist/index.js",
    "cli": "bun cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "lint-staged": {
    "*.{js,ts,svelte,json,css,md}": "prettier --write"
  },
  "bin": {
    "sk-template": "cli/dist/index.js"
  },
  "files": [
    "cli/dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.1",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.555.0",
    "@sveltejs/enhanced-img": "^0.8.5",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/table-core": "^8.21.3",
    "@types/bun": "latest",
    "bits-ui": "^2.14.4",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "layerchart": "^1.0.12",
    "lint-staged": "^16.2.7",
    "paneforge": "^1.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.45.6",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.4",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "@types/chalk": "^2.2.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "arctic": "^3.7.0",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "clsx": "^2.1.1",
    "flags": "^4.0.2",
    "inquirer": "^12.11.1",
    "iovalkey": "^0.3.3",
    "jsdom": "^27.2.0",
    "jsonwebtoken": "^9.0.3",
    "mode-watcher": "^1.1.0",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.4.0",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  }
}
